{
  "suggest.noselect": false,
  "coc.preferences.formatOnSave": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "json",
    "markdown",
    "html",
    "css",
    "scss"
  ],
  "eslint.enable": true,
  "eslint.filetypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.options": {
    "configFile": "~/.eslintrc.json",
    "plugins": [],
    "rules": {}
  },

  "prettier.enable": true,
  "prettier.eslintIntegration": true,
  "prettier.singleQuote": true,
  "tsserver.enable": true,
  "tsserver.useWorkspaceDependencies": true
}
